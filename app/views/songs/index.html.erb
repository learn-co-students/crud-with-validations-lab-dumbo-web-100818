<h1>All Songs</h1>
<table>
  <thead>
        <tr>
            <th>Song #</th>
            <th scope="col">Title</th>
            <th scope="col">Released</th>
            <th scope="col">Release Year</th>
            <th scope="col">Artist Name</th>
            <th scope="col">Genre</th>
        </tr>
    </thead>
<tbody>
  <% @songs.each_with_index do |song, i| %>
   <tr>
      <th scope="row"><%= i+1 %>.</th>
      <td><%= song.title %></td>
      <td><%= song.released %></td>
      <td><%= song.release_year %></td>
      <td><%= song.artist_name %></td>
      <td><%= song.genre %></td>
   </tr>
  <% end %>
</tbody>
</table>
