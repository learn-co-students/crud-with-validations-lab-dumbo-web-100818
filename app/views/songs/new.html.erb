<%=form_for Song.new do |f|%>
<%=f.label :title%>
<%=f.text_field :title%>
<%=f.label :artist_name%>
<%=f.text_field :artist_name%>
<%=f.label :release_year%>
<%=f.number_field :release_year%>
<%=f.label :released%>
<%=f.text_field :released%>
<%=f.label :genre%>
<%=f.text_field :genre%>
<%=f.submit%>
<%end%>
